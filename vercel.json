{
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/server.js"
    }
  ],
  
  "functions": {
    "src/.js": {
      "maxDuration": 15 // This function can run for a maximum of 15 seconds
    },
  }
}
